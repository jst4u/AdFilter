<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="TlPjUserRelation.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="TlPjUserRelation.pjId" />
				<ref bean="TlPjUserRelation.userId" />
				<ref bean="TlPjUserRelation.relationship" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="TlPjUserRelation.purId" parent="fieldDefinition">
		<property name="fieldName" value="purId" />
		<property name="label" value="系统ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="TlPjUserRelation.pjId" parent="fieldDefinition">
		<property name="fieldName" value="pjId" />
		<property name="label" value="项目ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="TlPjUserRelation.userId" parent="fieldDefinition">
		<property name="fieldName" value="userId" />
		<property name="label" value="人员" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="TlPjUserRelation.relationship" parent="fieldDefinition">
		<property name="fieldName" value="relationship" />
		<property name="label" value="关系" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="1" />
		<property name="width" value="100" />
	</bean>

	<bean id="TlPjUserRelation.creator" parent="fieldDefinition">
		<property name="fieldName" value="creator" />
		<property name="label" value="creator" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="TlPjUserRelation.createTime" parent="fieldDefinition">
		<property name="fieldName" value="createTime" />
		<property name="label" value="createTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="TlPjUserRelation.modifier" parent="fieldDefinition">
		<property name="fieldName" value="modifier" />
		<property name="label" value="modifier" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="TlPjUserRelation.modifyTime" parent="fieldDefinition">
		<property name="fieldName" value="modifyTime" />
		<property name="label" value="modifyTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="TlPjUserRelation.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="recVer" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

</beans>
