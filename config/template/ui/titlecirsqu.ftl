<!--titlecirsqu 开始  -->
<span class="walle_titlecirsqu walle_titlecirsquwidth"  id="${id}" name="${name}" style="height:${height};width:${width};${class};">
	<div id="walle_titlecirsqu_left_top"></div>
	<div id="walle_titlecirsqu_left_bottom"></div>
	<div id=walle_titlecirsqu_main>
		<div id="walle_titlecirsqu_title">
			<div id="walle_titlecirsqu_title_left"><img src="${imgsrc}" height="18px" width="18px" /></div>
			<div id="walle_titlecirsqu_title_center"><a href="${titlehref}" target="${target}">${title}</a></div>
			<div id="walle_titlecirsqu_title_right"><a  href="${titlehref}" style="color: #8ea1b5;" target="_parent">${righttitle}</a></div>
		</div>
		<div id="walle_titlecirsqu_body" style="height:100%;">
			${value}
		</div>
	</div>
	<div id="walle_titlecirsqu_right_top"></div>
	<div id="walle_titlecirsqu_right_bottom"></div>
</span>
<!--titlecirsqu 结束-->