<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="AppNodeBase.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="AppNodeBase.nodeId" />
				<ref bean="AppNodeBase.nodeState" />
				<ref bean="AppNodeBase.installationPosition" />
				<ref bean="AppNodeBase.installationPersonnel" />
				<ref bean="AppNodeBase.installationTime" />
				<ref bean="AppNodeBase.systemTime" />
				<ref bean="AppNodeBase.remarks1" />
				<ref bean="AppNodeBase.remarks2" />
				<ref bean="AppNodeBase.remarks3" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="AppNodeBase.id" parent="fieldDefinition">
		<property name="fieldName" value="id" />
		<property name="label" value="ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.nodeId" parent="fieldDefinition">
		<property name="fieldName" value="nodeId" />
		<property name="label" value="节点ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.nodeState" parent="fieldDefinition">
		<property name="fieldName" value="nodeState" />
		<property name="label" value="节点状态" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="29" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.installationPosition" parent="fieldDefinition">
		<property name="fieldName" value="installationPosition" />
		<property name="label" value="安装位置" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="300" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.installationPersonnel" parent="fieldDefinition">
		<property name="fieldName" value="installationPersonnel" />
		<property name="label" value="安装人员" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="100" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.installationTime" parent="fieldDefinition">
		<property name="fieldName" value="installationTime" />
		<property name="label" value="安装时间" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="AppNodeBase.systemTime" parent="fieldDefinition">
		<property name="fieldName" value="systemTime" />
		<property name="label" value="系统时间" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="AppNodeBase.remarks1" parent="fieldDefinition">
		<property name="fieldName" value="remarks1" />
		<property name="label" value="备注1" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="100" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.remarks2" parent="fieldDefinition">
		<property name="fieldName" value="remarks2" />
		<property name="label" value="备注2" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="100" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.remarks3" parent="fieldDefinition">
		<property name="fieldName" value="remarks3" />
		<property name="label" value="备注3" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="100" />
		<property name="width" value="200" />
	</bean>

	<bean id="AppNodeBase.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="版本号" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

</beans>
