<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="SysRoleFunc.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="SysRoleFunc.roleId" />
				<ref bean="SysRoleFunc.funcId" />
				<ref bean="SysRoleFunc.state" />
				<ref bean="SysRoleFunc.remarks" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="SysRoleFunc.roleFuncId" parent="fieldDefinition">
		<property name="fieldName" value="roleFuncId" />
		<property name="label" value="ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysRoleFunc.roleId" parent="fieldDefinition">
		<property name="fieldName" value="roleId" />
		<property name="label" value="角色" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysRoleFunc.funcId" parent="fieldDefinition">
		<property name="fieldName" value="funcId" />
		<property name="label" value="功能" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysRoleFunc.state" parent="fieldDefinition">
		<property name="fieldName" value="state" />
		<property name="label" value="状态" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="1" />
		<property name="width" value="100" />
	</bean>

	<bean id="SysRoleFunc.remarks" parent="fieldDefinition">
		<property name="fieldName" value="remarks" />
		<property name="label" value="备注" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="1000" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysRoleFunc.creator" parent="fieldDefinition">
		<property name="fieldName" value="creator" />
		<property name="label" value="creator" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysRoleFunc.createTime" parent="fieldDefinition">
		<property name="fieldName" value="createTime" />
		<property name="label" value="createTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="SysRoleFunc.modifier" parent="fieldDefinition">
		<property name="fieldName" value="modifier" />
		<property name="label" value="modifier" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysRoleFunc.modifyTime" parent="fieldDefinition">
		<property name="fieldName" value="modifyTime" />
		<property name="label" value="modifyTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="SysRoleFunc.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="recVer" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

</beans>
