<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="SysCodeType.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="SysCodeType.codeTypeName" />
				<ref bean="SysCodeType.codeTypeCode" />
				<ref bean="SysCodeType.codeTypeDesc" />
				<ref bean="SysCodeType.codeTypeType" />
				<ref bean="SysCodeType.remarks" />
				<ref bean="SysCodeType.dynColValue" />
