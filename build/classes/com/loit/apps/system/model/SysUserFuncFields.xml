<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="SysUserFunc.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="SysUserFunc.userId" />
				<ref bean="SysUserFunc.funcId" />
				<ref bean="SysUserFunc.state" />
				<ref bean="SysUserFunc.remarks" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="SysUserFunc.userFuncId" parent="fieldDefinition">
		<property name="fieldName" value="userFuncId" />
		<property name="label" value="userFuncId" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysUserFunc.userId" parent="fieldDefinition">
		<property name="fieldName" value="userId" />
		<property name="label" value="userId" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysUserFunc.funcId" parent="fieldDefinition">
		<property name="fieldName" value="funcId" />
		<property name="label" value="funcId" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysUserFunc.state" parent="fieldDefinition">
		<property name="fieldName" value="state" />
		<property name="label" value="state" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="1" />
		<property name="width" value="100" />
	</bean>

	<bean id="SysUserFunc.remarks" parent="fieldDefinition">
		<property name="fieldName" value="remarks" />
		<property name="label" value="remarks" />
		<property name="fieldType" value="text" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysUserFunc.creator" parent="fieldDefinition">
		<property name="fieldName" value="creator" />
		<property name="label" value="creator" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysUserFunc.createTime" parent="fieldDefinition">
		<property name="fieldName" value="createTime" />
		<property name="label" value="createTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="SysUserFunc.modifier" parent="fieldDefinition">
		<property name="fieldName" value="modifier" />
		<property name="label" value="modifier" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysUserFunc.modifyTime" parent="fieldDefinition">
		<property name="fieldName" value="modifyTime" />
		<property name="label" value="modifyTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="SysUserFunc.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="recVer" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

</beans>
