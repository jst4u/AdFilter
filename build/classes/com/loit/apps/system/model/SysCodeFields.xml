<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="SysCode.ALL" parent="fieldListDefinition">
		<constructor-arg>
			<list>
				<ref bean="SysCode.codeTypeId" />
				<ref bean="SysCode.codeName" />
				<ref bean="SysCode.codeValue" />
				<ref bean="SysCode.codeDesc" />
				<ref bean="SysCode.codeOrder" />
				<ref bean="SysCode.remarks" />
			</list>
		</constructor-arg>
	</bean>

	<bean id="SysCode.codeId" parent="fieldDefinition">
		<property name="fieldName" value="codeId" />
		<property name="label" value="代码ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="false" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.codeTypeId" parent="fieldDefinition">
		<property name="fieldName" value="codeTypeId" />
		<property name="label" value="代码类型ID" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.codeName" parent="fieldDefinition">
		<property name="fieldName" value="codeName" />
		<property name="label" value="代码类型名称" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.codeValue" parent="fieldDefinition">
		<property name="fieldName" value="codeValue" />
		<property name="label" value="编码类型代号" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.codeDesc" parent="fieldDefinition">
		<property name="fieldName" value="codeDesc" />
		<property name="label" value="代码类型描述" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.codeOrder" parent="fieldDefinition">
		<property name="fieldName" value="codeOrder" />
		<property name="label" value="codeOrder" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="110" />
	</bean>

	<bean id="SysCode.remarks" parent="fieldDefinition">
		<property name="fieldName" value="remarks" />
		<property name="label" value="备注说明" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="200" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.creator" parent="fieldDefinition">
		<property name="fieldName" value="creator" />
		<property name="label" value="creator" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.createTime" parent="fieldDefinition">
		<property name="fieldName" value="createTime" />
		<property name="label" value="createTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="SysCode.modifier" parent="fieldDefinition">
		<property name="fieldName" value="modifier" />
		<property name="label" value="modifier" />
		<property name="fieldType" value="string" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="length" value="36" />
		<property name="width" value="200" />
	</bean>

	<bean id="SysCode.modifyTime" parent="fieldDefinition">
		<property name="fieldName" value="modifyTime" />
		<property name="label" value="modifyTime" />
		<property name="fieldType" value="datetime" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="width" value="190" />
	</bean>

	<bean id="SysCode.recVer" parent="fieldDefinition">
		<property name="fieldName" value="recVer" />
		<property name="label" value="recVer" />
		<property name="fieldType" value="int" />
		<property name="sortable" value="true" />
		<property name="nullable" value="true" />
		<property name="precision" value="10" />
		<property name="width" value="100" />
	</bean>

</beans>
