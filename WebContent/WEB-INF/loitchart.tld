<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>loitchart</short-name>
	<uri>/loitchart</uri>
	<display-name>loitchart</display-name>
	<tag>
		<name>chart</name>
		<tag-class>com.loit.core.web.tag.highchart.ChartTag</tag-class>
		<body-content>JSP</body-content>
		<description>标签描述：图表选项</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>id属性</description>
		</attribute>
		<attribute>
			<name>type</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				默认的系列类型图。可以是 line, spline, area, areaspline, column, bar, pie, scatter.
			</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表的主标题</description>
		</attribute>
		<attribute>
			<name>subtitle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表的副标题</description>
		</attribute>
		<attribute>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表宽度，默认根据图表容器自适应宽度</description>
		</attribute>
		<attribute>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表高度，默认根据图表容器自适应高度</description>
		</attribute>
		<attribute>
			<name>xAxisTitle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表的X轴名称</description>
		</attribute>
		<attribute>
			<name>yAxisTitle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表的Y轴名称</description>
		</attribute>
		<attribute>
			<name>xAxisType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>yAxis的类型
			</description>
		</attribute>
		<attribute>
			<name>yAxisTitleAlign</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>yAxis的水平对齐方式</description>
		</attribute>
		<attribute>
			<name>legendAlign</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图例在图表区域的水平对齐方式。有效值是"left", "center" and "right"&lt;br/&gt;默认是 center
			</description>
		</attribute>
		<attribute>
			<name>legendLayout</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>显示形式，支持水平horizontal和垂直vertical，&lt;br/&gt;默认值horizontal</description>
		</attribute>
		<attribute>
			<name>legendVerticalAlign</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图例在图表区域的水平对齐方式。可以是"top", "middle"或"bottom"&lt;br/&gt;默认是bottom
			</description>
		</attribute>
		<attribute>
			<name>legendBackgroundColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例背景色，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>alignTicks</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>多轴时最小刻度是否自动计算，&lt;br/&gt;默认值true.</description>
		</attribute>
		<attribute>
			<name>animation</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否启用动画效果，&lt;br/&gt;默认值true.</description>
		</attribute>
		<attribute>
			<name>backgroundColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				 图表外部的背景色或渐变色&lt;br/&gt;默认是  #FFFFFF.
			</description>
		</attribute>
		<attribute>
			<name>borderColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表外边框的颜色，&lt;br/&gt;默认值#4572A7</description>
		</attribute>
		<attribute>
			<name>borderRadius</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				 图表外边框的圆角半径&lt;br/&gt;默认是5.0
			</description>
		</attribute>
		<attribute>
			<name>borderWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>外边框的像素宽度&lt;br/&gt;默认值0.0</description>
		</attribute>
		<attribute>
			<name>className</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表容器div使用的一个css类名，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>defaultSeriesType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description> type的别名，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>events</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				 可以为addSeries 、click、load、redraw、selection&lt;br/&gt;默认是  null
			</description>
		</attribute>
		<attribute>
			<name>ignoreHiddenSeries</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>如果true,一旦一个系列被隐藏，轴将会扩展剩余的可见系列，&lt;br/&gt;默认值true</description>
		</attribute>
		<attribute>
			<name>inverted</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				是否倒置轴，使得x轴时垂直的y轴是水平的&lt;br/&gt;默认是false
			</description>
		</attribute>
		<attribute>
			<name>margin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表的外边框和绘图区域的边距&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>marginBottom</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description> 下边距，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>marginLeft</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>左边距，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>marginRight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				右边距&lt;br/&gt;默认是 null
			</description>
		</attribute>
		<attribute>
			<name>marginTop</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description> 上边距，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>pinchType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				相当于zoomType,但对于多点触控手势只。默认情况下,pinchType zoomType一样设置。然而在某些情况下,例如在股票图表在鼠标拖盘图表,而收聚是启用的。&lt;br/&gt;默认是null
			</description>
		</attribute>
		<attribute>
			<name>plotBackgroundColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>绘图区域的背景色或渐变&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>plotBackgroundImage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>绘图区背景图片的URL，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>plotBorderColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>绘图区的边框颜色，&lt;br/&gt;默认值#C0C0C0</description>
		</attribute>
		<attribute>
			<name>plotBorderWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				绘图区的边框宽度&lt;br/&gt;默认是 0.0
			</description>
		</attribute>
		<attribute>
			<name>plotShadow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>绘图区是否使用阴影效果，&lt;br/&gt;默认值false</description>
		</attribute>
		<attribute>
			<name>polar</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				极坐标&lt;br/&gt;默认是false
			</description>
		</attribute>
		<attribute>
			<name>reflow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description> 当窗口大小改变的时候，自适应div大小&lt;br/&gt;默认值true</description>
		</attribute>
		<attribute>
			<name>renderTo</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表可以被渲染的HTML元素，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>selectionMarkerFill</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				选中的图表区域的背景颜色&lt;br/&gt;默认是  rgba(69,114,167,0.25)
			</description>
		</attribute>
		<attribute>
			<name>shadow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否对外部图表应用下降阴影，&lt;br/&gt;默认值false</description>
		</attribute>
		<attribute>
			<name>showAxes</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				 是否显示最初的坐标轴&lt;br/&gt;默认是false
			</description>
		</attribute>
		<attribute>
			<name>spacingBottom</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表底部和内容的距离空间&lt;br/&gt;默认值15</description>
		</attribute>
		<attribute>
			<name>spacingLeft</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description> 图表左边和内容的距离空间，&lt;br/&gt;默认值10.0</description>
		</attribute>
		<attribute>
			<name>spacingRight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图表右边和内容的距离空间，&lt;br/&gt;默认值10.0</description>
		</attribute>
		<attribute>
			<name>spacingTop</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图表上边和内容的距离空间&lt;br/&gt;默认是 10.0
			</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>...，&lt;br/&gt;默认值horizontal</description>
		</attribute>
		<attribute>
			<name>zoomType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				额外的CSS样式可以给内部div容器&lt;br/&gt;默认是null
			</description>
		</attribute>
		<attribute>
			<name>exportingEnabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				是否使用该功能，当我false时，则图表没有导出及打印功能 &lt;br/&gt;默认是true 
			</description>
		</attribute>
		<attribute>
			<name>exportingFilename</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				导出图片文件的文件名，不包含后缀 &lt;br/&gt;默认是chart 
			</description>
		</attribute>
		<attribute>
			<name>exportingType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				导出图的类型，有image/png, image/jpeg, application/pdf可选&lt;br/&gt;默认是image/png.
			</description>
		</attribute>
		<attribute>
			<name>exportingUrl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				导出功能的服务器地址，导出功能需要相应的服务提供支持。你可以自己搭建服务器，在/exporting-server目录下有相应的源文件&lt;br/&gt;默认是http://export.highcharts.com
			</description>
		</attribute>
		<attribute>
			<name>exportingWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				导出图片文件的宽度，相应的，高度这按照比例 &lt;br/&gt;默认是800.0 
			</description>
		</attribute>
	</tag>
	<tag>
		<name>legend</name>
		<tag-class>com.loit.core.web.tag.highchart.LegendTag</tag-class>
		<body-content>JSP</body-content>
		<description>标签描述： 图例选项</description>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>id属性</description>
		</attribute>
		<attribute>
			<name>align</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图例在图表区域的水平对齐方式。有效值是"left", "center" and "right"&lt;br/&gt;默认是 center
			</description>
		</attribute>
		<attribute>
			<name>backgroundColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例背景色，&lt;br/&gt;默认值null</description>
		</attribute>
		<attribute>
			<name>borderColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例边框颜色&lt;br/&gt;默认值#909090</description>
		</attribute>
		<attribute>
			<name>borderRadius</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例边框角度&lt;br/&gt;默认值5</description>
		</attribute>
		<attribute>
			<name>enabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示图例&lt;br/&gt;默认值true</description>
		</attribute>
		<attribute>
			<name>floating</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否可以浮动，配合x，y属性&lt;br/&gt;默认值false</description>
		</attribute>
		<attribute>
			<name>itemDistance</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>itemDistance定义了每一项像素之间的距离。&lt;br/&gt;默认值8</description>
		</attribute>
		<attribute>
			<name>itemHiddenStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图例的每个系列项目或点项目隐藏时的CSS样式&lt;br/&gt;默认值itemHiddenStyle: {color:'#CCC'}&lt;br/&gt;填写规则：itemHiddenStyle="属性名1:属性值1,属性名2:属性值2"
			</description>
		</attribute>
		<attribute>
			<name>itemHoverStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图例的每个系列项目或点项目鼠标滑过时的CSS样式&lt;br/&gt;默认itemHoverStyle: {color: '#000'}&lt;br/&gt;填写规则：itemHoverStyle="属性名1:属性值1,属性名2:属性值2"
			</description>
		</attribute>
		<attribute>
			<name>itemMarginBottom</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>每个图例项底部边距的像素。&lt;br/&gt;默认值0</description>
		</attribute>
		<attribute>
			<name>itemMarginTop</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>每个图例项顶部边距的像素。&lt;br/&gt;默认值0</description>
		</attribute>
		<attribute>
			<name>itemStyle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				每个图例项的CSS样式。&lt;br/&gt;默认itemStyle: {cursor: 'pointer',color: '#274b6d',fontSize:'12px'}&lt;br/&gt;填写规则：itemStyle="属性名1:属性值1,属性名2:属性值2"
			</description>
		</attribute>
		<attribute>
			<name>itemWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>每个图例项目的宽度。当一个包含多个图例项目的水平图例需要垂直对齐时，这是非常有用的&lt;br/&gt;默认null</description>
		</attribute>
		<attribute>
			<name>labelFormat</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>系列的格式化标签。&lt;br/&gt;默认null</description>
		</attribute>
		<attribute>
			<name>layout</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例项目的布局。可以是’horizontal’或’vertical’&lt;br/&gt;默认值horizontal</description>
		</attribute>
		<attribute>
			<name>lineHeight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例项的行高&lt;br/&gt;默认值为16。</description>
		</attribute>
		<attribute>
			<name>lgMargin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>如果绘图区是自动计算且图例是不浮动的，图例的边距就是图例和坐标轴标签或绘图区的空间&lt;br/&gt;默认值为 15.0</description>
		</attribute>
		<attribute>
			<name>maxHeight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例的最大高度&lt;br/&gt;默认值为null</description>
		</attribute>
		<attribute>
			<name>padding</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例盒子的内边距&lt;br/&gt;默认值为 8.0。</description>
		</attribute>
		<attribute>
			<name>reversed</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例项目的顺序相对于系列或点定义的顺序是否要逆序&lt;br/&gt;默认值为false</description>
		</attribute>
		<attribute>
			<name>rtl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否把标志放在文本的右边&lt;br/&gt;默认值为 false</description>
		</attribute>
		<attribute>
			<name>shadow</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示阴影&lt;br/&gt;默认值false</description>
		</attribute>
		<attribute>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>图例的CSS样式。&lt;br/&gt;默认值null&lt;br/&gt;填写规则：style="属性名1:属性值1,属性名2:属性值2"</description>
		</attribute>
		<attribute>
			<name>symbolPadding</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				图例内标志和文本的距离&lt;br/&gt;默认是5
			</description>
		</attribute>	
		<attribute>
			<name>symbolWidth</name>
			<required>false</required>
