		</attribute>
		<attribute>
			<name>gridLineWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>绘图区网格线的宽度&lt;br/&gt;Type:Number &lt;br/&gt;默认值0</description>
		</attribute>
		<attribute>
			<name>id</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
