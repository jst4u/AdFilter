		</attribute>
		<attribute>
			<name>maxZoom</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>已过期。V2.2版本后改名为minRange.&lt;br/&gt;Type: Number &lt;br/&gt;</description>
		</attribute>
		<attribute>
