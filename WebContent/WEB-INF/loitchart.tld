		</attribute>
		<attribute>
			<name>lineColor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description> 轴本身的线的颜色。&lt;br/&gt;Type:Color &lt;br/&gt;默认值#C0D0E0</description>
		</attribute>
		<attribute>
			<name>lineWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>轴本身的线的宽度。&lt;br/&gt;Type: Number &lt;br/&gt;默认为0</description>
		</attribute>
		<attribute>
			<name>linkedTo</name>
			<required>false</required>
